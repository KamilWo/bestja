<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <template id="new_contactus" inherit_id="website.contactus" name="Contact us">
            <xpath expr="//div[@id='wrap']" position="replace">
                <div id="wrap">
                    <div class="container">
                        <h1 class="text-center">Kontakt</h1>
                        <div class="row">
                            <div class="col-md-8">
                                <div class="oe_structure">
                                    <div>
                                        <h2>Dane teleadresowe</h2>
                                        <p>Uniwersyteckie Centrum Wolontariatu</p>
                                        <p>ul. Dobra 56/66</p>
                                        <p class="mb16">00-312 Warszawa</p>
                                    </div>
                                    <div class="contact_us_tel_email">
                                        <p>tel. <span class='mt8 mb8'>22 55 27 201</span></p>
                                        <p>tel. <span class='mt8 mb8'>22 55 27 204</span></p>                                      
                                        <p>e-mail: wolontariat[at]uw.edu.pl</p>                                      
                                    </div>
                                    <div>
                                        <h2>Odwiedź nasze biur w BUW</h2>
                                        <p>Znajdziesz nas na parterze w alei głównej, w głębi za Galerią Plakatu - tuż obok Restauracji Fenomenalna.</p>                                                        
                                    </div>
                                </div>
                            </div>                            
                        </div>
                        <div class="row mt32 mb32">
                            <div class="col-md-11">
                                <iframe width="100%" height="300" frameborder="0" style="border:0" 
                                        src="https://www.google.com/maps/embed/v1/place?q=biblioteka%20uniwersytetu%20warszawskiego%2C%20Dobra%2C%20Warszawa%2C%20Polska&amp;key=AIzaSyDXLm2GV33XW1ETxLy7Sc97YVMfIWolBO0">
                                </iframe> 
                            </div>
                        </div>
                    </div>
                </div>
            </xpath>
        </template>
        <delete id="website.menu_contactus" model="website.menu"/>
    </data>
</openerp>